{
  "manifest_version": 2,
  "short_name": "Shopify Metafields Editor",
  "name": "Shopify Metafields Editor",
  "description": "Edit Shopify Metafields without any app for free!",
  "author": "Husseyexplores",
  "version": "0.1.8",
  "browser_action": {
    "default_icon": "/img/128.png",
    "default_popup": "/popup.html"
  },
  "icons": {
    "16": "/img/16.png",
    "48": "/img/48.png",
    "128": "/img/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.myshopify.com/admin", "https://*.myshopify.com/admin/", "https://*.myshopify.com/admin/*", "https://*.myshopify.com/admin/*/*"],
      "css": ["/static/css/bundle.css"],
      "js": ["/static/js/bundle.js"]
    }
  ],
  "web_accessible_resources":[
    "/static/css/bundle.css"
  ]
}
